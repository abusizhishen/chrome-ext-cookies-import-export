  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "antd": "^5.24.1",
    "bootstrap": "^5.3.3",
    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
